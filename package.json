{
  "name": "weather-api",
  "version": "1.0.0",
  "description": "simple implementation of a weather service",
  "main": "handler.js",
  "scripts": {
    "setup": "./node_modules/.bin/sls config credentials --provider aws --key KEY --secret SECRET",
    "start": "./node_modules/.bin/sls offline start -r eu-central-1 --noTimeout --corsDisallowCredentials false --noStart --stage local",
    "debug": "export SLS_DEBUG=* && node --inspect ./node_modules/.bin/sls offline start -r eu-central-1 --noTimeout --corsDisallowCredentials false --noStart --stage local",
    "deploy": "./node_modules/.bin/sls deploy --verbose --stage dev"
  },
  "author": "darionou",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.801.0",
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-winston": "^4.0.5",
    "fast-safe-stringify": "^2.0.7",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.1",
    "serverless-http": "^2.6.0",
    "triple-beam": "^1.3.0",
    "winston": "^3.3.3",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "serverless": "^2.14.0",
    "serverless-offline": "^6.8.0"
  }
}
